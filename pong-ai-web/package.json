{
  "name": "pong-ai-web",
  "version": "1.0.0",
  "description": "Interactive web companion for SGDK Pong AI - featuring browser-based game, neural network training, and AI visualization",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "build-wasm": "emcc src/wasm/pong.c -o public/pong.js -s WASM=1 -s EXPORTED_RUNTIME_METHODS='[\"ccall\",\"cwrap\"]' -s EXPORTED_FUNCTIONS='[\"_main\",\"_update_game\",\"_get_ball_x\",\"_get_ball_y\",\"_get_player1_y\",\"_get_player2_y\",\"_get_score1\",\"_get_score2\",\"_set_ai_mode\",\"_handle_input\"]' -s MODULARIZE=1 -s EXPORT_NAME='PongModule' -O3"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "@tensorflow/tfjs": "^4.10.0",
    "@tensorflow/tfjs-vis": "^1.5.1",
    "chart.js": "^4.4.0",
    "react-chartjs-2": "^5.2.0",
    "framer-motion": "^10.16.0",
    "lucide-react": "^0.263.1",
    "tailwindcss": "^3.3.3",
    "autoprefixer": "^10.4.15",
    "postcss": "^8.4.28",
    "react-hot-toast": "^2.4.1",
    "file-saver": "^2.0.5",
    "canvas-confetti": "^1.6.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "postcss": "^8.4.39",
    "tailwindcss": "^3.4.6",
    "vite": "^5.3.4"
  },
  "keywords": [
    "pong",
    "ai",
    "neural-network",
    "tensorflow",
    "webassembly",
    "react",
    "sgdk",
    "genesis",
    "retro-gaming"
  ],
  "author": "SGDK Pong AI Project",
  "license": "MIT"
}
